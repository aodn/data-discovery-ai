{
  "preprocessor": {
    "vocabs": [
      "AODN Instrument Vocabulary",
      "AODN Discovery Parameter Vocabulary",
      "AODN Platform Vocabulary"
    ],
    "rare_label_threshold": 10,
    "test_size": 0.2,
    "n_splits": 5,
    "train_test_random_state": 42
  },
  "keywordModel": {
    "dropout": 0.3,
    "learning_rate": 0.001,
    "fl_gamma": 2.0,
    "fl_alpha": 0.7,
    "epoch": 100,
    "batch": 32,
    "early_stopping_patience": 5,
    "reduce_lr_patience": 5,
    "validation_split": 0.1,
    "confidence": 0.4,
    "top_N": 2
  }
}
